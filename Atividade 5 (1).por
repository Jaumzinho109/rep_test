programa {
  cadeia nome
  real salario
  funcao inicio() {
    escreva("Olá, prazer em te conhecer! Por favor, informe o seu nome ")
    leia(nome)
    escreva("Agora, o seu salário, por gentileza ")
    leia(salario)
    escreva("O funcionário ", nome, " possui um salário de ", salario, " em fevereiro")
  }
}
